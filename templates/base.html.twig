bouclecomptes:
while (i < 100){
  i++;

  let togg = document.getElementById("togg"+i);
  let pwd = document.getElementById("pwd"+i);
  let btogg = document.getElementById("btogg"+i);
  let adetail = document.getElementById("adetail"+i);
  let ctogg = document.getElementById("ctogg"+i);
  let bdetail = document.getElementById("bdetail"+i);
  let cdetail = document.getElementById("cdetail"+i);

if (togg !== null){
  togg.addEventListener("click",function() {
    if (getComputedStyle(pwd).display != "none"){
      pwd.style.display = "none";
    } else {
      pwd.style.display = "block";
    }})}


if (btogg !== null){
      btogg.addEventListener("click",function() {
        if (getComputedStyle(adetail).display != "none"){
          adetail.style.display = "none";
        } else {
          adetail.style.display = "block";
        }})}

        if (ctogg !== null){

          ctogg.addEventListener("click",function() {
            if (getComputedStyle(cdetail).display != "none"){
              cdetail.style.display = "none";
              bdetail.style.display = "none";
            } else {
              cdetail.style.display = "block";
              bdetail.style.display = "block";
            }})
          continue;
        }

        }


